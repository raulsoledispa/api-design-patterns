{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier" : "error",
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ]
    }
}
